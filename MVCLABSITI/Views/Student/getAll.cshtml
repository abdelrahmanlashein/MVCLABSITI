<table class="table">
    <thead>
        <tr>
            <th scope="col">SSN</th>
            <th scope="col">Name</th>
            <th scope="col">Age</th>
            <th scope="col">Address</th>
            <th scope="col">Image</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var s in Model)
        {
            <tr>
                <th scope="row">@s.SSN</th>
                <td>@s.Name</td>
                <td>@s.Age</td>
                <td>@s.Address</td>
                <td>@s.Image</td>
                <td>@s.Email</td>
            </tr>
        }
    </tbody>
</table>