@model List<Department>
<table class="table">
    <thead>
        <tr>
            <th scope="col">DeptId</th>
            <th scope="col">Name</th>
            <th scope="col">ManagerName</th>
            <th scope="col">Location</th>
            <th scope="col">Branches</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var d in Model)
        {
            <tr>
                <th scope="row">@d.DeptId</th>
                <td>@d.Name</td>
                <td>@d.ManagerName</td>
                <td>@d.Location</td>
                <td>@d.Branch</td>
            </tr>
        }
    </tbody>
</table>